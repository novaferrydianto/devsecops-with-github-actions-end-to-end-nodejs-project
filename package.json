{
  "name": "quick-test-example",
  "version": "1.0.0",
  "type": "module",
  "main": "app.js",
  "scripts": {
    "dev": "node app.js",
    "start": "node app.js",
    "start:secure": "AIKIDO_BLOCK=true node app.js",
    "test": "mocha --reporter mocha-sonarqube-reporter --reporter-options output=xunit.xml test --exit",
    "cover": "cross-env NODE_OPTIONS=--experimental-vm-modules nyc --reporter=text --reporter=lcov mocha --exit",
    "precover": "rm -rf .nyc_output coverage",
    "postinstall": "node fix-mocha-exit.js"
  },
  "author": "Nova Ferrydianto <novaferrydianto@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "chai": "^6.2.1",
    "chai-as-promised": "^8.0.2",
    "cross-env": "^10.1.0",
    "mocha": "^11.7.5",
    "mocha-sonarqube-reporter": "1.0.2",
    "mochawesome": "^7.1.3",
    "mockery": "^2.1.0",
    "nyc": "^17.1.0",
    "sinon": "^21.0.0",
    "source-map-support": "^0.5.21"
  },
  "dependencies": {
    "@aikidosec/firewall": "^1.8.2",
    "colors": "^1.4.0",
    "command-line-args": "^6.0.1",
    "dotenv": "^17.2.3",
    "node-fetch": "^3.3.2"
  },
  "nyc": {
    "all": true,
    "include": [
      "*.js",
      "src/**/*.js"
    ],
    "exclude": [
      "test",
      "node_modules"
    ],
    "reporter": [
      "text",
      "lcov",
      "html"
    ],
    "report-dir": "coverage",
    "extension": [
      ".js",
      ".mjs"
    ],
    "require": [
      "source-map-support/register"
    ],
    "instrument": true,
    "hookRequire": true,
    "hookRunInContext": true,
    "hookRunInThisContext": true
  }
}
